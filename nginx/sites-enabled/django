server {

    listen 80;
    server_name not.configured.example.com;
    charset utf-8;

    location /static {
        alias /data/web/slj_web/static;
    }

    location / {
        proxy_pass http://web:8000;
        proxy_set_header Host ;
        proxy_set_header X-Real-IP ;
        proxy_set_header X-Forwarded-For ;
    }

}
